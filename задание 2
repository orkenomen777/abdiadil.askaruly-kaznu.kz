public class MainApp {
    public static void main(String[] args) {
        int[] array = new int[5]; // Создаем массив длиной 5
        fillArrayWithNumber(7, array); // Заполняем массив числом 7

        // Выводим заполненный массив
        for (int num : array) {
            System.out.print(num + " ");
        }
    }

    public static void fillArrayWithNumber(int number, int[] arr) {
        for (int i = 0; i < arr.length; i++) {
            arr[i] = number; // Заполняем каждую ячейку массива указанным числом
        }
    }
}
